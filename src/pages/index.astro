---
import UiLayout from "@/components/layout/UiLayout.astro";
import Post from "@/features/feeds/posts/components/post.astro";

import { rssProvider } from "@/features/feeds/providers/rss";

const feed = await rssProvider({
	feedUrl: "https://scouting.org/feed/",
});
---

<UiLayout>
	<div class="flex justify-center p-8">
		<div class="flex max-w-5xl flex-col justify-stretch gap-5 w-full">
			{feed.map((item) => <Post post={item} />)}
		</div>
	</div>
</UiLayout>
