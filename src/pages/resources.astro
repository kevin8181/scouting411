---
import UiLayout from "@/components/layout/UiLayout.astro";
import { getResources } from "@/features/resources";
import { Link } from "@/features/resources/components/Link";
---

<UiLayout>
	<div class="p-6">
		<div class="flex flex-col gap-5">
			{
				(await getResources()).map((resource) => {
					return <Link resource={resource} />;
				})
			}
		</div>
	</div>
</UiLayout>
